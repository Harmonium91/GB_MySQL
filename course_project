#------- В данном блоке приведены DDL-команды -------#
#-----------------------------------------------------
#-----------------------------------------------------

create database railway;

use railway;

drop table if exists trains;
create table trains(
id serial,
stamp varchar(50),
van int,
registration_at datetime default now()
); -- в таблице хранятся сведения о поездах

drop table if exists crew;
create table crew(
id serial,
type_crew varchar(200) 
);  --  в таблице хранятся сведения о бригадах

drop table if exists staff;
create table staff(
id serial,
surname varchar(50) not null,
name varchar(50) not null,
patronymic varchar(50),
birthday date not null,
gender enum('male', 'female'),
specialization enum('machinist', 'mechanic', 'conductor', 'chief'),
phone bigint unsigned unique,
license varchar(10) not null unique,
crew_id bigint unsigned not null,

foreign key (crew_id) references crew(id) 
on update cascade on delete no action
); -- в таблице хранятся сведения о сотрудниках железнодорожной бригады

drop table if exists medical_exam;
create table medical_exam(
id serial,
date_exam datetime,
valid_until datetime,
staff_id bigint unsigned not null,

foreign key (staff_id) references staff(id) 
on update cascade on delete no action
); -- в таблице хранятся сведения о пройденных и действующих медицинских обследованиях у сотрудников

drop table if exists passengers;
create table passengers(
id serial,
surname varchar(50) not null,
name varchar(50) not null,
patronymic varchar(50),
birthday date not null,
gender enum('male', 'female'),
phone bigint unsigned unique,
email varchar(100) unique
); -- в таблице хранятся сведения о пассажирах

drop table if exists stations;
create table stations(
id serial,
station_name varchar(40),
code varchar(3)
); -- в таблице хранятся сведения о станциях

drop table if exists routes;
create table routes(
id serial,
departure_station_id bigint unsigned not null,
arrival_station_id bigint unsigned not null,
distance int comment 'unit of measurement kilometers',
price decimal(10,2),
created_at datetime default now(),
updated_at datetime on update current_timestamp,

foreign key (departure_station_id) references stations(id) 
on update cascade on delete no action,
foreign key (arrival_station_id) references stations(id)
on update cascade on delete no action
); -- в таблице хранятся сведения о маршрутах и их стоимости

drop table if exists `assignment`;
create table `assignment`(
id serial,
train_id bigint unsigned not null,
crew_id bigint unsigned not null,
created_at datetime default now(),
updated_at datetime on update current_timestamp,
status enum('current', 'cancelled'),

foreign key (train_id) references trains(id) 
on update cascade on delete no action,
foreign key (crew_id) references crew(id)
on update cascade on delete no action
); -- в таблице хранятся сведения о назначениях бригад на поезда

drop table if exists schedule;
create table schedule(
id serial,
train_id bigint unsigned not null,
route_id bigint unsigned not null,
departure_at datetime,
arrival_at datetime,
track_number char(1) not null,

foreign key (train_id) references trains(id) 
on update cascade on delete no action,
foreign key (route_id) references routes(id)
on update cascade on delete no action
); -- в таблице хранится расписание поездов

drop table if exists discounts;
create table discounts(
id serial,
category varchar(50),
amount float
); -- в таблице хранятся сведения о льготах

drop table if exists additional_services;
create table additional_services(
id serial,
service_type varchar(50),
cost decimal(10,2)
); -- в таблице хранятся сведения о дополнительных услугах

drop table if exists tickets;
create table tickets(
id serial,
schedule_id bigint unsigned not null,
passenger_id bigint unsigned not null,
discount_id bigint unsigned not null,
additional_service_id bigint unsigned not null,
created_at datetime default now(),
total_cost decimal(10,2),

foreign key (schedule_id) references schedule(id) 
on update cascade on delete no action,
foreign key (passenger_id) references passengers(id)
on update cascade on delete no action,
foreign key (discount_id) references discounts(id) 
on update cascade on delete no action,
foreign key (additional_service_id) references additional_services(id)
on update cascade on delete no action
); -- в таблице хранятся сведения о приобретенных билетах


alter table staff
add index staff_surname_name_birthday_patronymic_idx (surname, name, patronymic, birthday);

alter table passengers
add index passengers_surname_name_birthday_patronymic_idx (surname, name, patronymic, birthday);

alter table stations
add index stations_name_idx (station_name);

alter table routes
add index routes_price_idx (price);


#----------- В данном блоке приведены запросы -----------
#--------------------------------------------------------
#--------------------------------------------------------

-- 1.Возвращает сведения о пассажирах
select concat(surname, ' ', name, ' ', patronymic) as FIO, 
	   gender, birthday, email 
from passengers; 

-- 2.Возвращает список машинистов из общего числа персонала
select surname, name, birthday, license
from staff 
where specialization = 'machinist'; 

-- 3.Возвращает данные о медицинском обследовании выбранного сотрудника
select surname, name, birthday,
	   (select date_exam from medical_exam where staff_id = 1),
	   (select valid_until from medical_exam where staff_id = 1)
from staff
where id = 1;

-- 4.Возвращает сведения о приобретенных пассажирами билетах
select concat(surname, ' ', name) as FIO, 
	   count(t.id) as total, sum(t.total_cost) as `sum`
from passengers p join tickets t
on p.id = t.passenger_id
group by surname; 

-- 5.Возвращает среднюю стоимость билета в указанный промежуток времени
select avg(total_cost) as `sum`
from tickets
where created_at between '2000-01-01' and '2022-01-01';

-- 6.Возвращает билеты, стоимость которых меньше минимальной стоимости маршрутов
select id, total_cost
from tickets 
where total_cost < (
					select min(price)
					from routes
				   );
				  
-- 7.Возвращает ТОП 3 наиболее популярных дополнительных услуг
select a.id, a.service_type, count(t.id) as total 
from additional_services a
	join tickets t on a.id = t.additional_service_id
group by a.service_type
order by total desc 
limit 3;

-- 8.Возвращает список персонала с просроченным медицинским обследованием
select concat(surname, ' ', name, ' ', patronymic) as FIO, 
	   birthday, valid_until
from staff 
	join medical_exam on staff.id = staff_id 
where valid_until < now();

-- 9.Возвращает список наиболее задействованных сотрудников
select concat(surname, ' ', name) as FIO, count(a.id) as total_assignment
from staff s 
	join crew c on s.crew_id = c.id 
	join `assignment` a on c.id = a.crew_id
group by FIO 
having total_assignment = (
		select max(total)
		from(
			 select count(id) as total
			 from `assignment`
			 group by crew_id
			 ) as list
		 					)
order by total_assignment desc;

-- 10.Возвращает список несовершеннолетних пассажиров
select concat(surname, ' ', name) as FIO, 
	   timestampdiff(year, birthday, now()) as age
from passengers
where timestampdiff(year, birthday, now()) < 18;


-- 11.Возвращает наиболее востребованный маршрут
select r.id, r.price, s1.station_name as departure, s2.station_name as arrival, count(t.id) as total_tickets
from routes r
	join stations s1 on s1.id=r.departure_station_id
	join stations s2 on s2.id=r.arrival_station_id
	join schedule sc on r.id = sc.route_id 
	join tickets t on sc.id = t.schedule_id
group by t.schedule_id having total_tickets =
					(
					select max(total)
					from (
						  select count(id) as total 
						  from tickets
						  group by schedule_id) as list 
					);

				
#----------- В данном блоке приведены представления, триггеры, ХП и функции -----------
#--------------------------------------------------------------------------------------
#--------------------------------------------------------------------------------------

-- Представление отображает расписание в удобном, расширенном варианте
create or replace view full_shedule (train_id, departure_station, arrival_station, 
						  departure_at, arrival_at, track_number) as
	select train_id, st1.station_name, st2.station_name, 
		   departure_at, arrival_at, track_number
	from schedule s
		join routes r on s.route_id = r.id
		join stations st1 on r.departure_station_id = st1.id
		join stations st2 on r.arrival_station_id = st2.id;
		
-- Представление отображает перечень услуг и льгот, которыми пользуются пассажиры
create or replace view passengers_discounts_and_services (full_name, age, service_type, category) as
	select concat(surname, ' ', name, ' ', patronymic),
		   timestampdiff(year, birthday, now()), service_type, category
	from passengers 
		join tickets on passengers.id = passenger_id
		join additional_services on tickets.additional_service_id = additional_services.id
		join discounts on tickets.discount_id = discounts.id;

-- Данная хранимая процедура расчитывает стоимость 
-- билета с учетом тарифа, скидок и дополнительных услуг 
drop procedure if exists calculation_total_cost;
create procedure calculation_total_cost ()
begin
	set @start_value = 1; 
	set @count_tickets = (select count(id) from tickets);
		while @start_value < @count_tickets do
		set @cost = (select r.price + r.price * d.amount + ads.cost
			   	     from routes r
				  	   join schedule s on r.id = s.route_id 
				  	   join tickets t on s.id = t.schedule_id
				  	   join additional_services ads on ads.id = t.additional_service_id
				  	   join discounts d on d.id = t.discount_id
			   	     where t.id = @start_value);
		update tickets
		set total_cost = @cost
		where id = @start_value;
		set @start_value = @start_value + 1;
		end while;
end;

call calculation_total_cost()				

-- Данная функция аналогична предыдущей хранимой процедурой, но 
-- обновляет стоимость по заданному id билета

drop function if exists calc_total_cost;
create function calc_total_cost (ticket_id int)
returns int deterministic
begin 
	set @cost = (select truncate(r.price + r.price * d.amount + ads.cost, 2)
			     from routes r
				   join schedule s on r.id = s.route_id 
				   join tickets t on s.id = t.schedule_id
				   join additional_services ads on ads.id = t.additional_service_id
				   join discounts d on d.id = t.discount_id
			     where t.id = ticket_id);
	update tickets
	set total_cost = @cost
	where id = ticket_id;
return(@cost);
end;

select calc_total_cost(1);				
				
-- Данные триггеры осуществляют проверку возраста персонала при добавлении сотрудника

create trigger age_verification_insert before insert on staff
for each row 
	begin 
		if timestampdiff(year, new.birthday, now()) < 18 
		then signal sqlstate '45000' set message_text = 'Сотрудник не может быть младше 18 лет'; 
		end if;
end;

create trigger age_verification_update before update on staff
for each row 
	begin 
		if timestampdiff(year, new.birthday, now()) < 18 
		then signal sqlstate '45000' set message_text = 'Сотрудник не может быть младше 18 лет'; 
		end if;
end;


